@model List<ApplicationCore.Models.MovieCardResponseModel>

@{ 
    ViewData["Title"] = "Home Page";
}

<div class="rounded">
    <div class="row">
        @foreach (var movieCard in Model)
        {
            <div class="col-xl-2 col-sm-4 col-lg-3 col-6">
                @*<partial name="_Moviecard" model="movieCard" />*@

                @await Html.PartialAsync("_MovieCard", movieCard)
            </div>
            
        }
    </div>
</div>
